<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.pswd_manager">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/logo2"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/logo2"
            android:supportsRtl="true"
            android:theme="@style/Theme.Pswd_manager">
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" android:label="@string/app_name"/>
        <activity android:name=".AuthenticateActivity" android:label="@string/app_name"/>
        <activity android:name=".SetAuthenticatePasswordActivity" android:label="@string/app_name"/>
        <activity android:name=".SetSafetyQuestionsActivity" android:label="@string/app_name"/>
    </application>

</manifest>